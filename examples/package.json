{
  "name": "unidriver-examples",
  "version": "1.3.3",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build-ts": "tsc -p .",
    "build-rest": "cp src/index.ejs dist/index.ejs && browserify dist/client.js > dist/bundle.js",
    "build": "npm run build-ts && npm run build-rest",
    "test-jsdom": "mocha -r jsdom-global/register dist/**/*.spec.js --exit --timeout 20000",
    "test-e2e": "mocha dist/**/*.e2e.js --exit --timeout 20000",
    "test": "npm run test-jsdom && npm run test-e2e"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/puppeteer": "^5.4.0",
    "@unidriver/core": "^1.2.3",
    "@unidriver/jsdom-react": "^1.6.3",
    "@unidriver/puppeteer": "^1.2.2",
    "@unidriver/selenium": "^1.2.3",
    "browserify": "^16.0.0",
    "chromedriver": "^94.0.0",
    "express": "^4.16.0",
    "find-free-port-sync": "^1.0.0",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.0.0",
    "puppeteer": "^5.5.0"
  }
}
