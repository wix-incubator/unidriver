    "@types/express": "^4.11.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
